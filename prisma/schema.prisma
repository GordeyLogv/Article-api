generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ArticleModel {
  id Int @id @default(autoincrement())
  title String
  content String
  imageUrl String
  views Int @default(0)
  likes Int @default(0)
  createdAt DateTime @default(now())
}