generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ArticleModel {
  id Int @id @default(autoincrement())
  title String
  content String
  imageUrl String
  views Int
  likes Int
  createdAt DateTime
}

model UserModel {
  id Int @id @default(autoincrement())
  role String
  email String @unique
  password String
  name String
  age Int
  isBlocked Boolean
  createdAt DateTime
}